{
  "name": "oak-js",
  "version": "2.0.0-dev",
  "description": "An online assembler/assembler generator and simulator.",
  "main": "bin/Oak.js",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "prompt-sync": "^4.1.6",
    "readline-sync": "^1.4.9",
    "brace": "^0.11.1",
    "react": "^16.4.2",
    "react-ace": "^6.1.4",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-cli": "./node_modules/typescript/bin/tsc --module amd --pretty --target ES2016 --removeComments --noEmitOnError src/backend/main.ts --outFile bin/Oak.js",
    "start": "react-scripts start",
    "build": "./node_modules/typescript/bin/tsc --module amd --pretty --target ES2016 --removeComments --noEmitOnError src/backend/Zero.ts --outFile src/backend.js ; node -e \"let fs = require('fs');fs.appendFileSync('./src/backend.js', fs.readFileSync('./src/backend/ZeroPlus.js'));\" ; react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donn/Oak.js.git"
  },
  "keywords": [
    "assembly",
    "assembler",
    "riscv",
    "mips",
    "simulator"
  ],
  "contributors": [
    "Karim Abdel Hamid <karimah@aucegypt.edu> https://karimio.github.io/",
    "Mohamed Gaber <donnspersonal@icloud.com> https://donn.github.io/about.html",
    "Ali Khaled <alikhaled96@aucegypt.edu> https://github.com/alikhaled96"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/donn/Oak.js/issues"
  },
  "homepage": "https://github.com/donn/Oak.js#readme"
}
